---
import Layout from '../layouts/Layout.astro';
import IntroExercises from '../components/Section/IntroExercises.astro';
import Subtitulo from '../components/UI/Subtitulo.astro';
import Boton from '../components/UI/Boton.astro'

---

<Layout title="CodeMasterJS">
	
	<main class=" max-w-5xl mx-auto lg:h-[80vh] ">
		<!-- Seccion de titulos -->
		<section class="flex flex-col justify-center gap-12 my-9">
			<IntroExercises />
			<Subtitulo />

			 <div class=" flex flex-wrap gap-5 justify-center">
				<Boton  texto="Ejercicios js"  link={"/Ejercicios"}/>
				<Boton  texto="Pruebas Tecnicas" link={"/404"} />
			 </div>

		</section>

		
		
	</main>
</Layout>

